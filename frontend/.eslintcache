<form onSubmit={this.handleSubmit}>\n                    <label>\n                            Name\n                            <input name='name' type='text' value={this.state.name} \n                            onChange={this.handleChange} required={required} />                \n                    </label>\n\n                    <label>\n                            Email\n                            <input name='email' type='email' value={this.state.email} \n                            onChange={this.handleChange} required={required} />                \n                    </label>\n\n                    <label>\n                            Message\n                            <textarea name='message' rows='10' value={this.state.message} \n                            onChange={this.handleChange} required={required} />                \n                    </label>\n\n                    <input type='submit' value='Submit' />\n\n                    {alert}\n                    </form>\n            </section>\n            </main>\n        );\n    }\n}\n\n",["46","47"],"/Users/mica/Projects/Websites/micahorton/frontend/src/api.js",[],{"ruleId":"48","replacedBy":"49"},{"ruleId":"50","replacedBy":"51"},{"ruleId":"52","severity":1,"message":"53","line":4,"column":8,"nodeType":"54","messageId":"55","endLine":4,"endColumn":14},{"ruleId":"48","severity":1,"message":"56","line":50,"column":13,"nodeType":"54","messageId":"57","endLine":50,"endColumn":18},{"ruleId":"48","replacedBy":"58"},{"ruleId":"50","replacedBy":"59"},"no-native-reassign",["60"],"no-negated-in-lhs",["61"],"no-unused-vars","'Navbar' is defined but never used.","Identifier","unusedVar","Read-only global 'alert' should not be modified.","nativeReassign",["60"],["61"],"no-global-assign","no-unsafe-negation"]